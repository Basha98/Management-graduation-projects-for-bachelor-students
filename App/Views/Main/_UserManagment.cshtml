@model App.Models.UserManagment
@{
    ViewBag.Title = "FileFrom";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<hr />
<h3 style="text-align:center">Students Management</h3>
<hr />
<br />
<link href="~/AA/bootstrap.min.css" rel="stylesheet" />
<link href="~/AA/Site.css" rel="stylesheet" type="text/css" />

<script src="~/Scripts/jquery-1.6.4.min.js"></script>
<script src="~/xx/bootstrap.min.js"></script>
<div class="container">
    <table class="table  table-bordered   table-striped">
        <tr>

            <th>
                Student Name
            </th>


            <th>
                Delete
            </th>

            <th>
                Details
            </th>

        </tr>

        @foreach (var item in Model.StudentList)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.User.FullName)
                </td>


                <td>

                    @Html.ActionLink("Delete", "DeleteUser", new { id = item.Id, token = item.Token })

                </td>
                <td>

                    @Html.ActionLink("Details", "DetailsForStudent", new { id = item.Id, token = item.Token })

                </td>

            </tr>

        }


    </table>


    <div>
        @Html.ActionLink("To Back", "IndexAdmin", new { token = Model.Token }, new { @class = "btn btn-primary" })
    </div>
</div>


